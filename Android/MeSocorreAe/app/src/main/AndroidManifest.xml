<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mesocorreae">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".InformacoesApp"
        android:allowBackup="true"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".EditarPerfilActivity"
            android:label="@string/title_activity_editar_perfil"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.PrincipalActivity" />
        </activity>
        <activity
            android:name=".RecuperarSenhaActivity"
            android:label="@string/title_activity_recuperar_senha"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.LoginActivity" />
        </activity>
        <activity
            android:name=".AlterarSenhaActivity"
            android:label="@string/title_activity_alterar_senha"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.PrincipalActivity" />
        </activity>
        <activity
            android:name=".SolicitacaoActivity"
            android:label="@string/title_activity_solicitacao"
            android:parentActivityName=".PerfilActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.PerfilActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PerfilActivity"
            android:label="@string/title_activity_perfil"
            android:launchMode="singleTop"
            android:parentActivityName=".ListaAutonomosActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.ListaAutonomosActivity" />
        </activity>
        <activity
            android:name=".PedidoActivity"
            android:label="@string/title_activity_pedido"
            android:parentActivityName=".ListaPedidosActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.ListaPedidosActivity" />
        </activity>
        <activity
            android:name=".ListaPedidosActivity"
            android:label="@string/title_activity_lista_pedidos"
            android:parentActivityName=".PrincipalActivity"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.PrincipalActivity" />
        </activity>
        <activity
            android:name=".ListaAutonomosActivity"
            android:label="@string/title_activity_lista_autonomos"
            android:launchMode="singleTop"
            android:parentActivityName=".PrincipalActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.PrincipalActivity" />
        </activity>
        <activity
            android:name=".CadastroActivity"
            android:label="@string/title_activity_cadastro"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.mesocorreae.LoginActivity" />
        </activity>
        <activity
            android:name=".PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:launchMode="singleTop"
            android:theme="@style/NoActionBarTransp" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>